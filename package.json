{
	"name": "webpack-4-boilerplate",
	"version": "1.0.0",
	"description": "webpack-4-boilerplate",
	"main": "index.js",
	"scripts": {
		"build": "NODE_ENV=production webpack-cli",
		"start": "node ./server.js",
		"test": "jest"
	},
	"jest": {
		"setupFiles": [
			"./test/jestsetup.js"
		],
		"snapshotSerializers": [
			"<rootDir>/node_modules/enzyme-to-json/serializer"
		],
		"moduleNameMapper": {
			"^.+\\.(css|sass)$": "identity-obj-proxy"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/omelenetskiy/webpack-4-boilerplate.git"
	},
	"author": "Alex Omelenetskiy",
	"license": "MIT",
	"dependencies": {
		"babel-core": "6.26.3",
		"babel-loader": "7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"clean-webpack-plugin": "0.1.19",
		"copy-webpack-plugin": "4.5.1",
		"css-loader": "0.28.11",
		"express": "4.16.3",
		"firebase": "^4.13.1",
		"history": "^4.7.2",
		"mini-css-extract-plugin": "0.4.0",
		"node-sass": "4.9.0",
		"prop-types": "^15.6.1",
		"react": "16.4.0",
		"react-dom": "16.4.0",
		"react-hot-loader": "4.2.0",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.2.2",
		"react-scripts": "1.1.4",
		"redux": "^4.0.0",
		"redux-devtools-extension": "^2.13.2",
		"redux-thunk": "^2.2.0",
		"sass-loader": "7.0.1",
		"style-loader": "0.21.0",
		"webpack": "4.9.1",
		"webpack-cli": "2.1.4"
	},
	"devDependencies": {
		"babel-jest": "^23.0.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"enzyme": "^3.3.0",
		"file-loader": "^1.1.11",
		"jest": "^23.1.0",
		"react-test-renderer": "^16.4.1",
		"webpack-dev-middleware": "3.1.3",
		"webpack-hot-middleware": "2.22.2"
	}
}
